<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="summaryPane" prefHeight="150.0" prefWidth="250.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="application.OrderSummaryController">
   <children>
      <Button fx:id="OrderCancelBtn" layoutX="14.0" layoutY="111.0" mnemonicParsing="false" onAction="#OrderCancelBtnClick" text="Cancel my order" />
      <Button fx:id="OrderConfirmBtn" layoutX="125.0" layoutY="111.0" mnemonicParsing="false" onAction="#OrderConfirmBtnClick" text="Get me my coffee!" />
      <Label fx:id="OrdererNameLabel" layoutX="14.0" layoutY="14.0" text="Name" textFill="#612c8a">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="OrderDescriptionLabel" layoutX="44.0" layoutY="64.0" text="Order" textFill="#d73939">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
